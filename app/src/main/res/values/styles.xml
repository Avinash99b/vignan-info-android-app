<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="HomeBlockLayoutStyle" parent="">
        <item name="cornerSize">5%</item>
    </style>
    <style name="HomePrevVisitedSystemStyle" parent="">
        <item name="cornerSize">50%</item>
    </style>
    <style name="AddChipCustomStyle" parent="Widget.MaterialComponents.Chip.Choice">
        <item name="chipBackgroundColor">@color/black</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:checkable">true</item>
        <item name="checkedIconVisible">true</item>
        <item name="checkedIconTint">@color/white</item>
        <item name="chipIconVisible">false</item>
        <item name="chipStartPadding">@dimen/_7sdp</item>
        <item name="chipEndPadding">@dimen/_7sdp</item>
    </style>
    <style name="FloorChipStyle" parent="Widget.MaterialComponents.Chip.Choice">
        <item name="chipStrokeWidth">1dp</item>
        <item name="chipStrokeColor">@color/black</item>
        <item name="android:checkable">true</item>
        <item name="checkedIconVisible">false</item>
        <item name="chipIconVisible">false</item>
        <item name="chipStartPadding">@dimen/_7sdp</item>
        <item name="chipEndPadding">@dimen/_7sdp</item>
    </style>

    <style name="DashboardSystemChipStyle" parent="Widget.MaterialComponents.Chip.Choice">
        <item name="chipStrokeWidth">1dp</item>
        <item name="chipStrokeColor">@color/black</item>
        <item name="android:checkable">true</item>
        <item name="checkedIconVisible">false</item>
        <item name="chipIconVisible">true</item>
        <item name="chipStartPadding">@dimen/_7sdp</item>
        <item name="chipEndPadding">@dimen/_7sdp</item>
        <item name="chipCornerRadius">7dp</item>
    </style>
</resources>